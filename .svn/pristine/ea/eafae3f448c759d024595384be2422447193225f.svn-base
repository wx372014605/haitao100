html,body{
	_background-image:url(about:blank);/*用浏览器空白页面作为背景*/
	_background-attachment:fixed;/* prevent screen flash in IE6 确保滚动条滚动时，元素不闪动*/
}
.hongfeng_disable_select{
	-moz-user-select: none; /*火狐*/
	-webkit-user-select: none; /*webkit浏览器*/
	-ms-user-select: none; /*IE10*/
	-khtml-user-select: none; /*早期浏览器*/
	user-select: none;
}
.hongfeng_clear{
	clear: both;	
}

/****插件分页样式****/
#hongfeng_page_main{font:12px/1.5 Tahoma,Helvetica,Arial,sans-serif;padding:6px;text-align:center;box-sizing:content-box; width:450px;}
#hongfeng_page_main *{box-sizing:content-box;}
#hongfeng_page_main a{color:#3366CC;outline:none;display:inline-block;height:20px;line-height:20px;vertical-align:middle;}
#hongfeng_page_main a:hover{border:1px solid #FD6D01;color:#FD6D01;text-decoration:none;}
#hongfeng_page_main span{display:inline-block;height:20px;line-height:20px;vertical-align:middle;}
#hongfeng_page_main .hongfeng_first_page{position:relative;margin:0 2px;border:1px solid #ccc;border-radius:4px;overflow:hidden;color:#404040;padding:2px 5px 2px 14px;}
#hongfeng_page_main .hongfeng_first_page i{position:absolute;left:2px;top:50%;margin-top:-4px;width:10px;height:10px;background:url('../images/plugin/page_ico.png') 0px -40px no-repeat;}
#hongfeng_page_main #hongfeng_first_page i{background-position:0 -50px;}
#hongfeng_page_main #hongfeng_first_page:hover{text-decoration:none;}
#hongfeng_page_main .hongfeng_pre_page{position:relative;margin:0 2px;border:1px solid #ccc;border-radius:4px;overflow:hidden;color:#404040;padding:2px 5px 2px 14px;}
#hongfeng_page_main .hongfeng_pre_page i{position:absolute;left:2px;top:50%;margin-top:-4px;width:10px;height:10px;background:url('../images/plugin/page_ico.png') 0px 0px no-repeat;}
#hongfeng_page_main #hongfeng_pre_page i{background-position:0 -10px;}
#hongfeng_page_main #hongfeng_pre_page:hover{text-decoration:none;}
#hongfeng_page_main .hongfeng_page_num{margin:0 2px;border:1px solid #ccc;border-radius:2px;overflow:hidden;padding:2px 8px;}
#hongfeng_page_main .hongfeng_page_num:hover{text-decoration:none;}
#hongfeng_page_main #hongfeng_current_page{margin:0 2px;border:1px solid #FD6D01;border-radius:2px;overflow:hidden;padding:2px 8px;background:#FFEDE1;color:#FD6D4E;font-weight:bold;}
#hongfeng_page_main .hongfeng_next_page{position:relative;margin:0 2px;border:1px solid #ccc;border-radius:4px;overflow:hidden;color:#404040;padding:2px 14px 2px 5px;}
#hongfeng_page_main .hongfeng_next_page i{position:absolute;right:2px;top:50%;margin-top:-4px;width:10px;height:10px;background:url('../images/plugin/page_ico.png') 0px -20px no-repeat;}
#hongfeng_page_main #hongfeng_next_page i{background-position:0 -30px;}
#hongfeng_page_main #hongfeng_next_page:hover{text-decoration:none;}
#hongfeng_page_main .hongfeng_last_page{position:relative;margin:0 2px;border:1px solid #ccc;border-radius:4px;overflow:hidden;color:#404040;padding:2px 14px 2px 5px;}
#hongfeng_page_main .hongfeng_last_page i{position:absolute;right:2px;top:50%;margin-top:-4px;width:10px;height:10px;background:url('../images/plugin/page_ico.png') 0px -60px no-repeat;}
#hongfeng_page_main #hongfeng_last_page i{background-position:0 -70px;}
#hongfeng_page_main #hongfeng_last_page:hover{text-decoration:none;}
#hongfeng_page_main #hongfeng_page_tip{margin:0 2px;}
#hongfeng_page_main #hongfeng_page_tip .hongfeng_now_page{color:#FD6D4E;font-weight:bold;}
/****插件分页样式****/

/****商品搜索框****/
#hongfeng_search_tip_main{position:absolute;left:0px;top:0px;z-index:99999;padding:6px 10px;color:#000;font-size:12px;font-weight:normal;background:#fff;border:2px solid #09A9D3;border-radius:5px;opacity:.8;filter:alpha(opacity=80);display:none;}
#hongfeng_search_tip_main:before{content:"";position:absolute;left:30px;top:100%;z-index:1;width:0;height:0;border-top:8px solid #fff;border-left:2px solid transparent;border-right:8px solid transparent;}
#hongfeng_search_tip_main:after{content:"";position:absolute;left:28px;top:100%;z-index:0;width:0;height:0;border-top:11px solid #09A9D3;border-left:3px solid transparent;border-right:12px solid transparent;}
#hongfeng_translate_result_main{position:absolute;left:0px;top:0px;z-index:99999;padding:6px 10px;background:#fff;border:2px solid #09A9D3;border-radius:5px;opacity:.8;filter:alpha(opacity=80);cursor:pointer;display:none;}
#hongfeng_translate_result_main:before{content:"";position:absolute;left:30px;top:100%;z-index:1;width:0;height:0;border-top:8px solid #fff;border-left:2px solid transparent;border-right:8px solid transparent;}
#hongfeng_translate_result_main:after{content:"";position:absolute;left:28px;top:100%;z-index:0;width:0;height:0;border-top:11px solid #09A9D3;border-left:3px solid transparent;border-right:12px solid transparent;}
#hongfeng_translate_result_main span{color:#000;font-size:12px;font-weight:bold;}
#hongfeng_translate_result_main font{margin-left:10px;font-size:12px;color:#0066C0;font-weight:normal;}

/****插件主界面****/
#hongfeng_plugin_container{
	position:fixed;right:50px;top:50px;z-index:10000;
	_position:absolute;/*IE6 用absolute模拟fixed*/
	_top:expression(documentElement.scrollTop+50+"px");/*IE6 动态设置top位置*/
}
#hongfeng_plugin_position{position:relative;}
#hongfeng_plugin_position img{cursor:pointer;}
#hongfeng_plugin_tag{}
#hongfeng_plugin_main{font-family:"Microsoft YaHei";box-sizing:content-box;}
#hongfeng_plugin_main *{box-sizing:content-box;}
#hongfeng_plugin_main p{margin:0;}
#hongfeng_plugin_main .hongfeng_plugin_text{
	padding:2px 5px;border:1px solid #D0D0D0;border-top-color:#bbb;width:90px;height:20px;border-radius:3px;font-size:12px;overflow:hidden;outline:none;
	box-shadow:0 1px 0 rgba(255,255,255,0.5),0 1px 0 rgba(0,0,0,0.07) inset;box-sizing: border-box;margin-right:5px;
}
#hongfeng_plugin_main .hongfeng_plugin_text:focus{border-color:#e49747;box-shadow:0 0 3px rgba(228,121,17,0.5),0 1px 0 rgba(0,0,0,0.07) inset;}
#hongfeng_plugin_main .hongfeng_button{
	display:inline-block;height:30px;line-height:30px;padding:0 20px;color:#fff;font-weight:bold;font-size:14px;
	background:url('../images/plugin/button_back.png');border:1px solid #F2863E;border-radius:5px;overflow:hidden;cursor:pointer;
}
#hongfeng_plugin_main .hongfeng_button:hover{color:#fff;text-decoration:none;background-position:0 -30px;}
#hongfeng_plugin_main .hongfeng_disable_button{background-position:0 -60px;border:1px solid #E5A234;}
#hongfeng_plugin_main .hongfeng_disable_button:hover{background-position:0 -60px;}
#hongfeng_plugin_main{font-size:12px;border:1px solid #337ab7;padding:10px 15px;border-radius:5px;margin:10px 0;box-shadow:1px 1px 5px #71AFED;background:#fff;text-align:left;font-family:"微软雅黑";}
#hongfeng_plugin_main a{color:#0066C0;text-decoration:none;}
#hongfeng_plugin_main a:hover{color:#C45500;text-decoration:underline;}
#hongfeng_plugin_main .hongfeng_common_button{
