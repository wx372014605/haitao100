#fixedLayer {
	position: fixed;
	_position: absolute;
	right: 10px;
	_right: 10px;
	top: 30%;
	_top: expression(eval(document.documentElement.scrollTop+document.documentElement.clientHeight-parseInt(document.documentElement.clientHeight/3)-this.offsetHeight-(parseInt(this.currentStyle.marginTop,10)||0)-(parseInt(this.currentStyle.marginBottom,10)||0)));
