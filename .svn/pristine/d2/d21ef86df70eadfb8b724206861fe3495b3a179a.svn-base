        // 产品菜单
        $('.item60').prepend('<span class="divider">|</span>').hover(
            function() {
                $(this).children('ul').show();
            },
            function() {
                $(this).children('ul').hide();
            }
        );

        $(function() {
            // 去除链接虚线
            $('a,input[type="button"],input[type="submit"]').bind('focus', function() {
                if (this.blur) {
                    this.blur();
                }
            });

            // 滚动菜单悬浮
            var isScroll = false;
            $(window).scroll(function() {

                if ($(this).scrollTop() > $('#header').height()) {
                    if (!isScroll) {
                        $('#header').css({
                            position: 'fixed',
                            top: '0px',
                            zIndex: '99',
                            opacity: '0.9'
                        });
                        isScroll = true;
                    }
                } else {
                    $('#header').css({
                        position: '',
                        top: '',
                        zIndex: '',
                        opacity: '1'
                    });
                    isScroll = false;
                }
            });
            // 底部合作伙伴和转运公司
            $('#partners-slider, #firms-slider').flexslider({
                //slideshow: false
            });

            $('#footer .slides .img').hover(
                function() {
                    $(this).css({
                        backgroundPosition: 'bottom center'
                    });
                },
                function() {
                    $(this).css({
                        backgroundPosition: 'top center'
                    });
                }
            );
        });
